#!/usr/bin/bash

processes=$(ps ax)

function execute_once {
    if ! echo $processes | grep $1 > /dev/null; then
        echo "Running $1"
        $@
    else
        echo "Skipping $1"
    fi
}

nitrogen --restore
sleep 1
compton-blur -b
execute_once ferdi
xset r rate 180 70
indicator-sound-switcher
